#include<bits/stdc++.h>
using namespace std;
bool lucky(int l)
{	bool z = true;
	int x;
	while (l > 0)
	{
		x = l % 10;
		if (x != 4 && x != 7)
		{
			z = false;
		}
		l = l / 10;

	}
	return z;
}
int main()
{

	int n;
	cin >> n;
	bool z;
	bool p = false;
	for (int i = 1; i <= n; i++)
	{
		z = lucky(i);
		if (z == true)
		{	if (n % i == 0)
			{

				p = true;

			}
		}
	}
	if (p == false)
		cout << "NO";
	else
		cout << "YES";
	return 0;




}
